    </Compile>
    <Compile Update="Views\NewStatusPage.xaml.cs">
      <DependentUpon>NewStatusPage.xaml</DependentUpon>
    </Compile>
    <Compile Include="MauiProgram.cs" />
  </ItemGroup>
